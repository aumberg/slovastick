<!-- <?xml version="1.0" encoding='UTF-8'?> -->
HTML
<e title="shortcuts for elements">
	<login 		s-see="//input[contains(@id, 	'login') and not(contains(@*, 'hidden'))]" s-title="login field"></login>
	<email 		s-see="//input[contains(@id, 	'email') and not(contains(@*, 'hidden'))]" s-title="email field"></email>
	<loginemail s-see-context=".." s-see="login|email"  s-title="login or email"></loginemail>
	<pass 		s-see="//input[contains(@id, 	'pass')  and not(contains(@*, 'hidden'))]" s-title="password field"></pass>
	<search 	s-see="//input[contains(@id,    'search')and not(contains(@*, 'hidden'))]" s-title="search on site"></search>
	<enter 		s-see="//button[(contains(@id, 	'login') or contains(@onclick, 	'login')) and not(contains(@*, 'hidden'))]"	s-title="enter to site"></enter>
	<vk 			s-go="http://vk.com" 			s-title="go to vkontakte"></vk>
	<!-- <music-yandex 	s-go="http://music.yandex.ru/" 	s-title="go to music yandex"></music-yandex> -->
</e>
<main s-null="">
	<div 		title="slovastick panel" 	id="slovastick_panel" 	style="position:fixed; left:10px; bottom:30px; width:300px; font-size:20px; z-index:2147483646; display:block;">
		<div 	title="slovastick result" 	id="slovastick_result"  style="background-color:#EEE;"> 
			focusing by press up + down arrows
		</div>
		<input 	title="slovastick console, focusing by shift + control" id="slovastick_console"  type="text" value="//slovastick" style="width:100%;"/>
	</div>
	<history 	title="history block for save program history">
		<global></global>
		<local></local>
		<temp></temp>
	</history>
</main>